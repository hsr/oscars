            <wsdl:input>
                <soap:body use="literal" parts="getStatusReq" />
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal" parts="getStatusResp" />
            </wsdl:output>
            <wsdl:fault name="OSCARSErrorException">
                <soap:fault name="OSCARSErrorException" use="literal" />
            </wsdl:fault>
        </wsdl:operation>


        <wsdl:operation name="getErrorReport">
            <soap:operation style="document"
                soapAction="http://oscars.es.net/OSCARS/coord/getErrorReport" />
            <wsdl:input>
                <soap:body use="literal" parts="getErrorReportReq" />
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal" parts="getErrorReportResp" />
            </wsdl:output>
            <wsdl:fault name=" OSCARSErrorException">
                <soap:fault name=" OSCARSErrorException" use="literal" />
            </wsdl:fault>
        </wsdl:operation>

        <wsdl:operation name="store">
            <soap:operation style="document"
                soapAction="http://oscars.es.net/OSCARS/resourceManager/store" />
            <wsdl:input>
                <soap:body use="literal" parts="storeReq" />
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal" parts="storeResp" />
            </wsdl:output>
            <wsdl:fault name="OSCARSErrorException">
                <soap:fault name="OSCARSErrorException" use="literal" />
            </wsdl:fault>
        </wsdl:operation>

        <wsdl:operation name="queryReservation">
            <soap:operation style="document"
                soapAction="http://oscars.es.net/OSCARS/resourceManager/queryReservation" />
            <wsdl:input>
                <soap:body use="literal" parts="queryReservationRequest" />
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal" parts="queryReservationResponse" />
            </wsdl:output>
            <wsdl:fault name="OSCARSErrorException">
                <soap:fault name="OSCARSErrorException" use="literal" />
