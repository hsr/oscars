<jxb:bindings version="1.0" xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:com.fnf="http://www.fnf.com/xes">
    <!-- Customize the package name that is generated for each schema.
  -->
    <jxb:bindings schemaLocation="nmtopo-ctrlp.xsd" node="/xs:schema">
        <jxb:schemaBindings>
            <jxb:package name="org.ogf.schema.network.topology.ctrlplane"/>
        </jxb:schemaBindings>
        <jxb:bindings node="//xs:complexType[@name='CtrlPlaneAddressContent']">
            <jxb:bindings node=".//xs:attribute[@name='value']">
                <jxb:property name="valueAttribute"/>
            </jxb:bindings>
        </jxb:bindings>
    </jxb:bindings>
</jxb:bindings>
