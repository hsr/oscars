       <wsdl:operation name="interDomainEvent">
            <soap:operation
                soapAction="http://oscars.es.net/OSCARS/interDomainEvent" />
            <wsdl:input>
                <soap:body use="literal" parts="interDomainEvent" />
            </wsdl:input>
        </wsdl:operation>
 
    </wsdl:binding>

        
    <wsdl:service name="OSCARSInternalService">
        <wsdl:port name="OSCARSInternalPortType" binding="tns:OSCARSSOAP">
        </wsdl:port>
    </wsdl:service>
</wsdl:definitions>
