<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="httpConfigProvider"
          class="net.es.oscars.nsibridge.config.http.JsonHttpConfigProvider"
          init-method="loadConfig"
          scope="singleton">
        <property name="filename" value="config/http.json"/>
    </bean>
    <bean id="oscarsConfigProvider"
          class="net.es.oscars.nsibridge.config.oscars.JsonOscarsConfigProvider"
          init-method="loadConfig"
          scope="singleton">
        <property name="filename" value="config/oscars.json"/>

    </bean>


</beans>